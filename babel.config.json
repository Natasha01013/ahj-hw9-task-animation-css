{
    "presets": [
      [
        "@babel/preset-env", //пресет, автоматически настраиваемый Babel для транспиляции кода в зависимости от целевых браузеров/окружений
        {
          "useBuiltIns": "entry", //опция указывает, как использовать полифилы. entry: будут добавляться через import
          "corejs": "3.22",
      "targets": {
        "node": "current" //код должен быть транспилирован для текущей версии Node.js, в которой выполняется проект
      }
        }
      ]
    ]
  }
